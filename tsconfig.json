{
  "compilerOptions": {
    "target": "es2017",
    "noImplicitAny": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "esModuleInterop": true,
    "lib": [
      "es2019",
      "dom"
    ],
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/__mocks__/*",
    "**/*.test.ts",
    "dist/**/*"
  ]
}